{
    "multipart": [
        {   "when": { "up": "true" },
            "apply": { "model": "gwycraft:dyed_cobblestone_wall_post_magenta" }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "gwycraft:dyed_cobblestone_wall_side_magenta", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "gwycraft:dyed_cobblestone_wall_side_magenta", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "gwycraft:dyed_cobblestone_wall_side_magenta", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "gwycraft:dyed_cobblestone_wall_side_magenta", "y": 270, "uvlock": true }
        }
    ]
}